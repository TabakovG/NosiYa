@model OutfitPartFormModel
@{
	ViewData["Title"] = "Добави елемент";
}

<h2 class="text-center">@ViewData["Title"]</h2>
<hr />

<div class="row">
	<div class="col-sm-12 offset-lg-2 col-lg-8 offset-xl-3 col-xl-6">
		<form enctype="multipart/form-data" method="post" asp-area="Admin" asp-controller="OutfitPart" asp-action="Add">
			<partial name="_OutfitPartFormPartial" model="@Model" />
		</form>
	</div>
</div>

@section Scripts
	{
	<partial name="_ValidationScriptsPartial" />
}