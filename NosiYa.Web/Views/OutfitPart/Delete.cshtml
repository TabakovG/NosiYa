@model OutfitPartForDelete

@{
	ViewBag.Title = "Изтрии елемент на носия";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<form method="post" asp-action="Delete">
	<div class="row">
		<div class="col-md-4">
			<img class="card-img-top" src="@Model.Images.FirstOrDefault()" alt="OutfitPart image...">
		</div>
		<div class="col-md-8">
			<div class="form-group">
				<label asp-for="Name" class="control-label"></label>
				<input asp-for="Name" class="form-control" disabled="disabled" />
			</div>
			<div class="form-group">
				<label asp-for="Description" class="control-label"></label>
				<input asp-for="Description" class="form-control" disabled="disabled" />
			</div>
			<div class="form-group">
				<label asp-for="OutfitPartType" class="control-label"></label>
				<input asp-for="OutfitPartType" class="form-control" disabled="disabled" />
			</div>
			<div class="form-group">
				<label asp-for="OwnerEmail" class="control-label"></label>
				<input asp-for="OwnerEmail" class="form-control" disabled="disabled" />
			</div>
			<div class="form-group">
				<label asp-for="RenterType" class="control-label"></label>
				<input asp-for="RenterType" class="form-control" disabled="disabled" />
			</div>
			<div class="form-group">
				<label asp-for="OutfitSetName" class="control-label"></label>
				<input asp-for="OutfitSetName" class="form-control" disabled="disabled" />
			</div>
			<div class="form-group mt-2">
				<input type="submit" value="Изтрии" class="btn btn-danger" />
			</div>
		</div>
	</div>
</form>